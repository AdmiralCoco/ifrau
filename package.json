{
  "name": "ifrau",
  "version": "0.0.1",
  "description": "Free-range app utility for IFRAME-based FRAs",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run clean && npm run transpile",
    "build": "browserify -s ifrau ./lib/index.js | uglifyjs > ./dist/ifrau.js",
    "clean": "rimraf dist && rimraf lib && mkdir dist",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "jshint src/*.js",
    "lint:test": "jshint test/*.js",
    "prepublish": "npm run build",
    "publish": "gulp publish",
    "test:unit": "babel-istanbul cover _mocha -- test/*.js",
    "test": "npm run lint -s && npm run test:unit -s",
    "transpile": "babel src --out-dir lib",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/ifrau.git"
  },
  "keywords": [
    "d2l",
    "frau",
    "iframe",
    "postMessage"
  ],
  "author": "D2L Corporation",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/ifrau/issues"
  },
  "homepage": "https://github.com/Brightspace/ifrau",
  "dependencies": {
    "q": "^1.4.1"
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-istanbul": "^0.2.4",
    "browserify": "^10.2.3",
    "chai": "^3.0.0",
    "coveralls": "^2.11.2",
    "gulp": "^3.9.0",
    "gulp-frau-publisher": "^2.2.0",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "peanut-gallery": "^1.0.0",
    "rimraf": "^2.3.4",
    "sinon": "^1.15.1",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.4.23"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
